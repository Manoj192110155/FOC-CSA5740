#include <stdio.h>
#include <string.h>
int main() {
    char s[100];
    printf("Enter the string:\n");
    fgets(s, sizeof(s), stdin);
    size_t length = strlen(s);
    if (length > 0 && s[length - 1] == '\n') {
        s[length - 1] = '\0';
    }
    printf("Length of the string = %lu\n", strlen(s));
    return 0;
}
