#include <stdio.h>
int main() {
    int a[10][10],b[10][10],c[10][10],m,n,i,j,k;
    printf("enter the number of row and coloumns:");
    scanf("%d %d",&m,&n);
    for(i=0;i<m;i++){
        for(j=0;j<n;j++){
            scanf("%d %d",&a[i][j],&b[i][j]);
        }
    }
    for(i=0;i<m;i++){
        for(j=0;j<n;j++){
            for(k=0;k<m;k++){
                c[i][j]=a[i][k]*b[k][j];
            }
        }
    }
    for(i=0;i<m;i++){
        for(j=0;j<n;j++){
            printf("%d \n",c[i][j]);
        }
    }
}
